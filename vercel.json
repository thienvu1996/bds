{
  "version": 2,
  "routes": [
    { "src": "/about", "dest": "/public/about.html" },
    { "src": "/projects", "dest": "/public/projects.html" },
    { "src": "/contact", "dest": "/public/contact.html" },
    { "src": "/admin", "dest": "/public/admin.html" },
    { "src": "/thank-you", "dest": "/public/thank-you.html" },
    { "src": "/components/(.*)", "dest": "/public/components/$1" },
    { "src": "/js/(.*)", "dest": "/public/js/$1" },
    { "src": "/data/(.*)", "dest": "/public/data/$1" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" }
  ]
}
